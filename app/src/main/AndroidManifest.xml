<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.swcontest">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Swcontest">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".GameActivity"></activity>
        <activity android:name=".HealthActivity"></activity>
        <activity android:name=".VideoActivity"></activity>

        <activity android:name=".Game1Activity"></activity>
        <activity android:name=".Game2Activity"></activity>
        <activity android:name=".Game3Activity"></activity>
        <activity android:name=".Game4Activity"></activity>
        <activity android:name=".Game5Activity"></activity>
        <activity android:name=".Game6Activity"></activity>
        <activity android:name=".Game7Activity"></activity>

        <activity android:name=".Video1Activity"></activity>
        <activity android:name=".Video2Activity"></activity>
        <activity android:name=".Video3Activity"></activity>

    </application>

</manifest>